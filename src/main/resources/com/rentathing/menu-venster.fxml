        <AnchorPane prefHeight="115.0" prefWidth="640.0">
            <children>
                <Label fx:id="medewerkerLabel" alignment="CENTER" layoutX="200.0" layoutY="39.0" prefHeight="38.0" prefWidth="241.0" text="medewerker" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
